{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#footer-masculino",
      "vtex.menu@2.x:menu#footer-feminino",
      "vtex.menu@2.x:menu#footer-acessorio",
      "footer-spacer",
      "flex-layout.col#footer-icons"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 8,
      "paddingBottom": 3
    }
  },

  "flex-layout.col#footer-icons": {
    "children": [
      "social-networks",
      "image#hiring",
      "flex-layout.row#footer-2-desktop"
    ]
  },

  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "children": [
      "accepted-payment-methods"
    ],
    "props": {
      "blockClass": "payment-methods"
    }
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": [
        "MasterCard",
        "Visa",
        "Diners Club"
      ]
    }
  },
  "flex-layout.row#footer-3-desktop": {
    "children": [
      "rich-text#footer"
    ],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "© 2022 All Rights Reserved | Desenvolvido por | [Equipe Dreamscape](https://github.com/emenni/dreamscape-final-project)",
      "textPosition": "CENTER",
      "blockClass": "footer"
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile",
      "flex-layout.row#3-footer-mobile"
    ]
  },
  "flex-layout.row#3-footer-mobile": {
    "children": [
      "rich-text#footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": [
      "social-networks",
      "accepted-payment-methods",
      "flex-layout.row#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods"
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": [
      "vtex.menu@2.x:menu#footer-mobile"
    ]
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "© 2022 All Rights Reserved | Desenvolvido por | [Equipe Dreamscape](https://github.com/emenni/dreamscape-final-project)",
      "textPosition": "CENTER",
      "blockClass": "footer"
    }
  },
  "image#hiring": {
    "props": {
      "src": "https://dreamscape.vtexassets.com/assets/vtex.file-manager-graphql/images/0a35e21a-da34-4e72-959e-b07c1b9d46e8___5d1bbb78b8b2de0d5cc559ee62970afe.png",
      "blockClass": "logo-footer"
    }
  },
  "flex-layout.row#footer-mobile": {
    "children": [
      "image#hiring-mobile"
    ]
  },
  "image#hiring-mobile": {
    "props": {
      "src": "https://dreamscape.vtexassets.com/assets/vtex.file-manager-graphql/images/0a35e21a-da34-4e72-959e-b07c1b9d46e8___5d1bbb78b8b2de0d5cc559ee62970afe.png",
      "blockClass": "logo-footer-mobile1",
      "horizontalAlign": "right",
      "verticalAlign": "right"
    }
  }
}